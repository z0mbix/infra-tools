add_newline = false

[aws]
style = "yellow"
format = "on [$symbol$region]($style) "
force_display = true

[character]
format = "$symbol "

[directory]
truncation_length = 8

[git_commit]
only_detached = false

[hostname]
style = "bold bright-yellow"
ssh_only = false
ssh_symbol = ""
disabled = false

[java]
disabled = false

[python]
format = '[${symbol}${pyenv_prefix}(${version} )(\($virtualenv\) )]($style)'

[time]
disabled = false
format = "[$time]($style) "
style = "bold bright-blue"

[username]
show_always = true
style_user = "bold green"
style_root = "bold red"
format = "[$user]($style) on "

[env_var.APP_ENV]
style = "{{ starship_env_colour }}"
format = '\([$env_value]($style)\) '
